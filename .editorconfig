# https://EditorConfig.org

# top-most EditorConfig file
root = true

[*]
charset=utf-8
end_of_line=lf
insert_final_newline=true
indent_style=space
indent_size=2

[*.json]
indent_style=space
indent_size=2

[{*.sc,*.scala}]
indent_style=space
indent_size=2

[{*.yaml,*.yml}]
indent_style=space
indent_size=2




